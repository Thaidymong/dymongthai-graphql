    "knex": "^2.5.1",
    "mysql2": "^3.6.0",
    "nodemon": "^3.0.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/plugin-proposal-async-generator-functions": "^7.20.7",
